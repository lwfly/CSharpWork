<script type="text/javascript">
    var currentPage = 1;
    var baseUrl = 'https://blog.csdn.net/blogdevteam/category_8684681';
    var pageSize = 40 ;
    var listTotal = 107 ;

    function getAllUrl(page) {
        if (page > 1) {
            return baseUrl + "_" + page + ".html";
        }
        return baseUrl + ".html";
    }

    var alPag = new Paging();
    alPag.init({
        target: '#pageBox',
        pagesize: pageSize,
        count: listTotal,
        current: currentPage,
        firstTpl: '',
        lastTpl: '',
        callback: function (page, size, count) {
            var url = getAllUrl(page);
            location.href = url;
        }
    })
</script>
